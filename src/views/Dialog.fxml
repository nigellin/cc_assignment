<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" maxHeight="150.0" minHeight="150.0" prefHeight="-1.0" prefWidth="480.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.DialogController">
  <children>
    <VBox alignment="CENTER" maxHeight="100.0" minHeight="100.0" prefHeight="-1.0" prefWidth="100.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
      <children>
        <FlowPane alignment="CENTER_LEFT" hgap="20.0" prefHeight="-1.0" prefWidth="-1.0">
          <children>
            <ImageView fx:id="icon" fitHeight="48.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
              <FlowPane.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
              </FlowPane.margin>
            </ImageView>
            <Text fx:id="messageText" strokeType="OUTSIDE" strokeWidth="0.0" text="" wrappingWidth="320.0">
              <font>
                <Font size="15.0" />
              </font>
            </Text>
          </children>
        </FlowPane>
        <FlowPane alignment="TOP_RIGHT" hgap="10.0" maxWidth="400.0" prefHeight="-1.0" prefWidth="-1.0" VBox.margin="$x1">
          <children>
            <Button mnemonicParsing="false" onAction="#actionClickedCancel" text="Cancel">
              <graphic>
                <ImageView fitHeight="24.0" fitWidth="16.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@icons/cross.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Button mnemonicParsing="false" onAction="#actionClickedOk" text="Ok">
              <graphic>
                <ImageView fitHeight="24.0" fitWidth="16.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@icons/checkmark.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
          </children>
          <padding>
            <Insets fx:id="x1" />
          </padding>
        </FlowPane>
      </children>
      <padding>
        <Insets bottom="20.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
